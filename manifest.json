{
    "manifest_version": 2,
    "name": "Chrome Video Player",
    "description": "This plays videos in chrome.",
    "version": "1.0",
    "permissions": [
        "webRequest", "webRequestBlocking",
        "https://mail-attachment.googleusercontent.com/*"
    ],
    "background": {
        "scripts": ["video_player.js", "background.js"]
    },
    "browser_action": {
        "default_icon": "logo.png"
    }
}
