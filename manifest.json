{
    "manifest_version": 2,
    "name": "Chrome Video Player",
    "description": "This plays videos in chrome.",
    "version": "1.0",
    "permissions": [
       "webRequest",
        "https://mail-attachment.googleusercontent.com/"
    ],
    "background": [
        "persistent": false,
        "scripts": ["background.js"]
    ],
    "browser_action": {
        "default_icon": "icon.png"
    }
}
